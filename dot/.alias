# make sure our EXA shell command exists.
if [[ "$(command -v exa)" == "" ]] then
    alias la='ls -a'
    alias ll='ls -l'
    alias l='ls'
else
    alias ls='exa'
    alias la='exa -a'
    alias ll='exa -l'
    alias l='exa'
fi

alias dn='dotnet'
alias zyp='sudo zypper'
alias tabvi='vim -p'
alias tabvim='vim -p'
alias v='vim $(fzf)'
alias vzf='vim $(fzf)'

alias venv='python3 -m venv'
